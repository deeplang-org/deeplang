class Solver {
    constructor () {
    };

    public
    sig solve::Int -> Int
    fun solve (i) {
        if (i == 0) {
            return 0;
        } else if (i == 1) {
            return 1;
        } else {
            return fib(i - 1) + fib(i - 2);
        };
    };
};



sig main:: void -> void
fun main () {
    let solver = new Solver();
    solver.solve(10);
};
